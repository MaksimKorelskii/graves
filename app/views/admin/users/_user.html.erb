<tr>
  <th scope="row"><%= user.id %></th>
  <td><%= user.name %></td>
  <td><%= user.role %></td>
  <td><%= user.email %></td>
  <td><%= user.created_at %></td>
  <td>
    <%= link_to t('.edit'), edit_admin_user_path(user), class: 'btn btn-secondary btn-sm' %>
    <%= link_to t('.delete'), admin_user_path(user), class: 'btn btn-danger btn-sm',
      data: {method: :delete, confirm: t('global.dialog.you_sure')} %>
  </td>
</tr>
